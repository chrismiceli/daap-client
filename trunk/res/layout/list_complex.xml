<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content"
	android:orientation="vertical">
	<TextView android:id="@+id/list_complex_title"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceLarge" />
	<TextView android:id="@+id/list_complex_caption"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:paddingRight="44dp" android:textAppearance="?android:attr/textAppearanceSmall"
		android:layout_below="@id/list_complex_title" />
</RelativeLayout>
